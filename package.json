{
    "name": "meteor-desktop-plugin-test-suite",
    "version": "0.0.14",
    "engines": {
        "node": ">=4.0.0"
    },
    "description": "Few utils to ease out functional testing of Meteor Desktop plugins.",
    "main": "dist/suite.js",
    "scripts": {
        "prepublish": "npm run build",
        "build": "babel src --out-dir dist --source-maps inline --copy-files",
